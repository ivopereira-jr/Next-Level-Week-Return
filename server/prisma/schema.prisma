// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

// para adicionar o prisma npm i prisma -D e npm @prisma/client

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id String @id @default(uuid())

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
